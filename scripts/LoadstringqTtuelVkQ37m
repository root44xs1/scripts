local OpStuffFolder = MainTab:AddFolder("Op stuff (for farm)")
OpStuffFolder:AddSwitch("Anti Knockback", function(state)
    getgenv().antiKnockback = state
    LocalPlayer.CharacterAdded:Connect(function(char)
        if getgenv().antiKnockback then
            char.HumanoidRootPart.Anchored = true
            task.wait(0.1)
            char.HumanoidRootPart.Anchored = false
        end
    end)
end)
OpStuffFolder:AddSwitch("Auto Glitch (5M)", function(state)
    getgenv().autoGlitch5M = state
    task.spawn(function()
        while getgenv().autoGlitch5M and LocalPlayer.Character do
            ReplicatedStorage.rEvents.glitchEvent:FireServer("5M")
            task.wait(0.1)
        end
    end)
end)
OpStuffFolder:AddSwitch("Auto Glitch (1M)", function(state)
    getgenv().autoGlitch1M = state
    task.spawn(function()
        while getgenv().autoGlitch1M and LocalPlayer.Character do
            ReplicatedStorage.rEvents.glitchEvent:FireServer("1M")
            task.wait(0.1)
        end
    end)
end)
OpStuffFolder:AddSwitch("Auto Pushups with Rock (5M) and Auto Punch", function(state)
    getgenv().autoPushups5M = state
    task.spawn(function()
        while getgenv().autoPushups5M and LocalPlayer.Character do
            local punch = LocalPlayer.Backpack:FindFirstChild("Pushups") or LocalPlayer.Character:FindFirstChild("Pushups")
            if punch then
                punch.Parent = LocalPlayer.Character
                ReplicatedStorage.muscleEvent:FireServer("punch", "rightHand")
                ReplicatedStorage.muscleEvent:FireServer("punch", "leftHand")
                local rock = Workspace.machinesFolder:FindFirstChild("Punching Rock")
                if rock then
                    firetouchinterest(rock.Rock, LocalPlayer.Character.RightHand, 0)
                    firetouchinterest(rock.Rock, LocalPlayer.Character.LeftHand, 0)
                    task.wait()
                    firetouchinterest(rock.Rock, LocalPlayer.Character.RightHand, 1)
                    firetouchinterest(rock.Rock, LocalPlayer.Character.LeftHand, 1)
                end
            end
            task.wait(0.1)
        end
    end)
end)
OpStuffFolder:AddButton("Remove Ad Portal", function()
    for _, portal in pairs(Workspace:GetDescendants()) do
        if portal.Name:match("AdPortal") then
            portal:Destroy()
        end
    end
end)
OpStuffFolder:AddButton("Walk on Water", function()
    local waterPart = Instance.new("Part")
    waterPart.Parent = Workspace
    waterPart.Anchored = true
    waterPart.Position = Vector3.new(0, 0, 0)
    waterPart.Size = Vector3.new(1000, 1, 1000)
    waterPart.Transparency = 0.5
end)