local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local RunService = game:GetService("RunService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")
local VirtualUser = game:GetService("VirtualUser")

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/CAXAP26BKyCH/library/refs/heads/main/Uilibrary"))()
local Window = Library:AddWindow("Doca V1 Free Version", {
    main_color = Color3.fromRGB(255, 0, 0),
    min_size = Vector2.new(500, 600),
    can_resize = true
})

local MainTab = Window:AddTab("Main")
local MiscFolder = MainTab:AddFolder("Misc")
MiscFolder:AddButton("Auto Weight (Equip any tool)", function()
    task.spawn(function()
        while true do
            for _, tool in pairs(LocalPlayer.Backpack:GetChildren()) do
                if tool:IsA("Tool") then
                    tool.Parent = LocalPlayer.Character
                    ReplicatedStorage.muscleEvent:FireServer("rep")
                    task.wait(0.1)
                end
            end
            task.wait()
        end
    end)
end)
MiscFolder:AddButton("Claim All Chest V2", function()
    for _, chest in pairs(Workspace:GetDescendants()) do
        if chest.Name:match("Chest") then
            fireclickdetector(chest:FindFirstChild("ClickDetector"))
            task.wait(0.1)
        end
    end
end)
MiscFolder:AddLabel("---Script Hub---")
MiscFolder:AddButton("Permanent ShiftLock", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/MiniNoobie/ShiftLockx/main/Shiftlock-MiniNoobie"))()
end)
MiscFolder:AddLabel("---Time---")
MiscFolder:AddButton("Night", function()
    ReplicatedStorage.rEvents.changeTimeRemote:InvokeServer("Night")
end)
MiscFolder:AddButton("Morning", function()
    ReplicatedStorage.rEvents.changeTimeRemote:InvokeServer("Morning")
end)
MiscFolder:AddButton("Day", function()
    ReplicatedStorage.rEvents.changeTimeRemote:InvokeServer("Day")
end)
MiscFolder:AddLabel("----Farming----")
local AutoBrawlFolder = MainTab:AddFolder("Auto Brawl")
AutoBrawlFolder:AddSwitch("Auto Join Brawl (For Farming)", function(state)
    getgenv().autoBrawlFarm = state
    task.spawn(function()
        while getgenv().autoBrawlFarm do
            ReplicatedStorage.rEvents.joinBrawl:FireServer("Farm")
            task.wait(1)
        end
    end)
end)
AutoBrawlFolder:AddSwitch("Auto Join Brawl (God Mode)", function(state)
    getgenv().autoBrawlGod = state
    task.spawn(function()
        while getgenv().autoBrawlGod do
            ReplicatedStorage.rEvents.joinBrawl:FireServer("GodMode")
            task.wait(1)
        end
    end)
end)
AutoBrawlFolder:AddSwitch("Invisible Join Brawl", function(state)
    getgenv().invisBrawl = state
    task.spawn(function()
        while getgenv().invisBrawl do
            ReplicatedStorage.rEvents.joinBrawl:FireServer("Invisible")
            task.wait(1)
        end
    end)
end)
local AutoGymFolder = MainTab:AddFolder("Auto Gym")
AutoGymFolder:AddLabel("King Gym")
AutoGymFolder:AddSwitch("Auto Muscle King Lift", function(state)
    getgenv().automlking = state
    task.spawn(function()
        while getgenv().automlking and LocalPlayer.Character do
            local machine = Workspace.machinesFolder:FindFirstChild("Muscle King Lift")
            if machine then
                LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(machine.interactSeat.Position)
                ReplicatedStorage.rEvents.machineInteractRemote:InvokeServer("useMachine", machine)
                ReplicatedStorage.muscleEvent:FireServer("rep")
            end
            task.wait(0.1)
        end
    end)
end)
AutoGymFolder:AddSwitch("Auto Muscle King Bench", function(state)
