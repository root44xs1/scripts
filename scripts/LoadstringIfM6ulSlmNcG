local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/1882827/865357UI/refs/heads/main/U585467I", true))()

local window = library:AddWindow("EpicDeevv V2 | Made By Dan", {
    main_color = Color3.fromRGB(0, 0, 0),
    min_size = Vector2.new(650, 650),
    can_resize = true,
})

local Main = window:AddTab("Main")

local Folder = Main:AddFolder("Local Player")

local autoEatEnabled = false

local function eatProteinEgg()
    local player = game.Players.LocalPlayer
    local backpack = player:WaitForChild("Backpack")
    local character = player.Character or player.CharacterAdded:Wait()

    local egg = backpack:FindFirstChild("Protein Egg")
    if egg then
        egg.Parent = character
        pcall(function()
            egg:Activate()
        end)
    end
end

task.spawn(function()
    while true do
        if autoEatEnabled then
            eatProteinEgg()
            task.wait(1800)
        else
            task.wait(1)
        end
    end
end)


Folder:AddSwitch("Auto Eat Egg",function(state)
    autoEatEnabled = state
end)

Folder:AddLabel("|-----Time-----|").TextSize = 25

local timeDropdown = Folder:AddDropdown("Change Time", function(selection)
    local lighting = game:GetService("Lighting")
    
    if selection == "Night" then
        lighting.ClockTime = 0
    elseif selection == "Day" then
        lighting.ClockTime = 12
    elseif selection == "Midnight" then
        lighting.ClockTime = 6
    end
    
end)

timeDropdown:Add("Night")
timeDropdown:Add("Day")
timeDropdown:Add("Midnight")






Folder:AddLabel("|-----Auto Farm-----|").TextSize = 25

local repToggle = false

Folder:AddSwitch("Auto Farm (Equip Any tool)", function(state)
    repToggle = state
    while repToggle do
        local args = { "rep" }
        game:GetService("Players").LocalPlayer:WaitForChild("muscleEvent"):FireServer(unpack(args))
        task.wait(0.2)
    end
end)

local folder1 = Main:AddFolder("Tools")

local weightOn = false
folder1:AddSwitch("Weight", function(bool)
    weightOn = bool
    task.spawn(function()
        while weightOn do
            local tool = game.Players.LocalPlayer.Backpack:FindFirstChild("Weight")
            if tool then
                tool.Parent = game.Players.LocalPlayer.Character
            end
            task.wait(0.1)
        end
    end)
end)

local pushupsOn = false
folder1:AddSwitch("Pushups", function(bool)
    pushupsOn = bool
